{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/Frontend/frontend/context/chatcontext.tsx",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "name": "C#: Backend [http]",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/Backend/Backend.csproj",
            "launchConfigurationId": "TargetFramework=;http"
        },
        {
            "name": "C#: Backend [https]",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/Backend/Backend.csproj",
            "launchConfigurationId": "TargetFramework=;https"
        },
        {
            "name": "C#: Backend [https]",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/Backend/Backend.csproj",
            "launchConfigurationId": "TargetFramework=;https"
        }
    ]
}